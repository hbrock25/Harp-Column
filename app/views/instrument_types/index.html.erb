<h1>Listing instrument_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @instrument_types.each do |instrument_type| %>
  <tr>
    <td><%= instrument_type.name %></td>
    <td><%= link_to 'Show', instrument_type %></td>
